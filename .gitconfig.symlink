[editor]
	editor=vim
[diff]
	renames = true
[color]
	ui = auto
[push]
	default = current
[alias]
    amend = commit --amend
    s = status --short --branch --ignore-submodules=untracked
	pull = pull --ff-only
	last = log -1 HEAD
    master = checkout master
    d = diff
[core]
	hooksPath = /home/marrakchino/.dotfiles/_git/hooks
